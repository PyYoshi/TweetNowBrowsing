  "scripts": {
    "dev:webserver": "better-npm-run dev:webserver",
    "dev:build": "better-npm-run dev:build",
    "start": "run-p dev:webserver prettier-watch",
    "release": "better-npm-run release:build"
  },
  "betterScripts": {
    "dev:webserver": {
      "command": "node utils/webserver.js",
      "env": {
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": 3000
      }
    },
    "dev:build": {
      "command": "node utils/build.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "release:build": {
      "command": "node utils/build.js",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "devDependencies": {
    "@types/chrome": "^0.0.104",
    "@types/jquery": "^3.3.35",
    "@types/lodash.isnumber": "^3.0.6",
    "@types/lodash.isstring": "^4.0.6",
    "@types/lodash.template": "^4.4.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/twitter-text": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "archiver": "^4.0.1",
    "better-npm-run": "^0.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.5.2",
    "eslint": "^6.8.0",
    "file-loader": "^6.0.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.2.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "style-loader": "^1.1.4",
    "terser-webpack-plugin": "^2.3.5",
    "ts-loader": "^7.0.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-dev-server": "^3.10.3",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "dependencies": {
    "jquery": "^3.3.1",
    "lodash.isnumber": "^3.0.3",
    "lodash.isstring": "^4.0.1",
    "lodash.template": "^4.5.0",
